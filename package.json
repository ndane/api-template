{
  "name": "api-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "start": "npm run migrate && nodemon --exec babel-node app.js",
    "serve": "npm run build && npm run migrate && node dist/app.js"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "jade": "~1.11.0",
    "morgan": "^1.8.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.7.0",
    "serve-favicon": "~2.4.2",
    "sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-plugin-node": "^5.1.1",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1"
  }
}
